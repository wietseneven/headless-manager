{
  "name": "headless-manager-client",
  "version": "0.1.3",
  "description": "Client to submit logs and events to the Headless Manager",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
