  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wietseneven/headless-manger/issues"
  },
  "homepage": "https://github.com/wietseneven/headless-manger#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-plugin-jest": "^25.3.4",
    "jest": "^27.4.7",
    "prettier": "^2.3.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
