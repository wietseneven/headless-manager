{
  "kind": "collectionType",
  "collectionName": "messages",
  "info": {
    "singularName": "message",
    "pluralName": "messages",
    "displayName": "Message",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "level": {
      "type": "enumeration",
      "enum": [
        "error",
        "warn",
        "info",
        "http",
        "verbose",
        "debug",
        "silly"
      ],
      "required": true,
      "default": "info"
    },
    "message": {
      "type": "string",
      "maxLength": 1000
    },
    "app": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::app.app",
      "inversedBy": "messages"
    },
    "ip": {
      "type": "string"
    }
  }
}
